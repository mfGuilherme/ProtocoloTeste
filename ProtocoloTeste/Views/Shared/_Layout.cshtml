<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProtocoloTeste</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ProtocoloTeste.styles.css" asp-append-version="true" />
</head>
<body class="custom-background">
    <header class ="custom-background">
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light custom-background mb-3">
            <div class="container-fluid">
                <a class="navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">ProtocoloTeste</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="container">
                    <ul class="navbar-nav mr-auto d-flex">
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white button btn btn-danger" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white button btn btn-danger" asp-area="" asp-controller="Clientes" asp-action="Index">Cadastro de Cliente</a>
                        </li>
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white button btn btn-danger" asp-area="" asp-controller="Protocolo" asp-action="Index">Cadastro de Protocolo</a>
                        </li>
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white button btn btn-danger" asp-area="" asp-controller="ProtocoloFollows" asp-action="Index">Acompanhamento de Protocolo</a>
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white button btn btn-danger" asp-area="" asp-controller="StatusProtocoloes" asp-action="Index">Status de Protocolo</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="custom-background">
        <div class="container">
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
